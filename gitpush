package=menu
version="menu (2.1.6)"
figlet $package
echo " Set Remote... "
git remote set-url origin git@github.com:hacktrackgnulinux/$package.git
echo " Pull Request Github... "
git pull
echo " Git Add Local... "
git add -A
echo " Git Commit Local... "
git commit -am "Update $version"
echo " Git Push Local to Github... "
git push
echo " Berhasil... "
